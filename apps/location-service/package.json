{
  "name": "@vexl-next/location-service",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules yarn jest --watch",
    "build": "yarn typecheck && yarn build:esbuild",
    "build:esbuild": "node esbuild.config.js",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf ./dist",
    "format": "prettier --check \"**/*.{js,mjs,cjs,jsx,ts,tsx,md,json}\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "dev": "npx tsx -r dotenv/config src/index.ts",
    "start": "node dist"
  },
  "dependencies": {
    "@effect/experimental": "^0.19.2",
    "@effect/opentelemetry": "^0.34.38",
    "@effect/platform": "^0.59.1",
    "@effect/platform-node": "^0.54.2",
    "@effect/schema": "^0.68.25",
    "@opentelemetry/exporter-trace-otlp-http": "^0.50.0",
    "@opentelemetry/sdk-trace-node": "^1.23.0",
    "@opentelemetry/sdk-trace-web": "^1.23.0",
    "@sentry/esbuild-plugin": "^2.10.2",
    "@sentry/node": "^7.93.0",
    "@sentry/profiling-node": "^1.3.5",
    "@vexl-next/rest-api": "0.0.0",
    "@vexl-next/server-utils": "0.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.4.5",
    "dynamic-import-polyfill": "^0.1.1",
    "effect": "^3.5.5",
    "effect-http": "^0.74.0",
    "effect-http-node": "^0.16.1",
    "fast-check": "^3.17.1"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/source-map-support": "^0.5.10",
    "@vexl-next/esbuild": "0.0.0",
    "@vexl-next/eslint-config": "0.0.0",
    "@vexl-next/prettier-config": "0.0.0",
    "@vexl-next/tsconfig": "0.0.0",
    "esbuild": "^0.17.16",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.2"
  },
  "prettier": "@vexl-next/prettier-config"
}
