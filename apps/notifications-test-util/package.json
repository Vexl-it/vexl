    "@vexl-next/tsconfig": "0.0.0",
    "esbuild": "^0.17.16",
    "eslint": "^8.50.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
